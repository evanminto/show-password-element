{
  "name": "@evanminto/show-password-element",
  "version": "2.0.0",
  "description": "Web component for toggling visiblity of a password field",
  "source": "src/index.js",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "targets": {
    "default": {
      "source": "src/index.js",
      "outputFormat": "esmodule",
      "distDir": "dist",
      "isLibrary": true
    },
    "browser": {
      "source": "src/browser.js",
      "context": "browser",
      "distDir": "dist"
    },
    "baseStyles": {
      "source": "src/base.css",
      "distDir": "dist"
    },
    "overlayInlineEndStyles": {
      "source": "src/overlay-inline-end.css",
      "distDir": "dist"
    }
  },
  "repository": "git@github.com:evanminto/show-password-element.git",
  "author": "Evan Minto <evan.minto@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.9.3"
  },
  "scripts": {
    "build": "parcel build",
    "prepublish": "parcel build"
  }
}
